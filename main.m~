clear;
clf; close all;

%% 设定温度曲线绘制

x = 0:0.1:300;
y = 0:3000;
for i = 1:3001
    y(i) = Tfur(x(i));
end
figure
width = 800;
height = 400;
set(gcf,'position',[0,0,width,height])
red = '#EB4537';
green = '#55AF7B';
blue = '#4286F3';
box on
grid on
hold on
plot(x, y, 'Color',red, 'LineWidth', 1)
title('炉温设定温度')
xlabel('t (s)')
ylabel('T_{fur} (^{\circ}C)')

saveas(gcf,['pic/_',convertStringsToChars(location), ...
            '_',int2str(max(ConfirmedAll)),'.svg'])

